+  static void transOmpLoop_others(SgOmpClauseBodyStatement* target,  
+      SgVariableDeclaration* index_decl, SgVariableDeclaration* lower_decl,  SgVariableDeclaration* upper_decl, 
+      SgBasicBlock* bb1)
